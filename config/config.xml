<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
	<depends plugin-name="com.iver.cit.gvsig"/>
	<depends plugin-name="es.udc.cartolab.gvsig.navtable"/>
	<depends plugin-name="es.udc.cartolab.gvsig.users"/>
	<libraries library-dir="lib"/>
	<resourceBundle name="text"/>
	<extensions>
		
		<extension class-name="es.udc.cartolab.gvsig.fonsagua.FormsExtension" active="true">
			<tool-bar name="fonsagua">
				<action-tool icon="forms-icon" tooltip="open_form"
					action-command="" position="50"/>
			</tool-bar>
		</extension>
		
		<extension class-name="es.udc.cartolab.gvsig.fonsagua.FormsMenuExtension" active="true">
			<menu text="fonsagua_menu/abastecimientos"
				action-command="abastecimientos"
				position="1"/>
			<menu text="fonsagua_menu/amenazas"
				action-command="amenazas"
				position="2"/>
			<menu text="fonsagua_menu/areas_potenciales_riego"
				action-command="areas_potenciales_riego"
				position="3"/>
			<menu text="fonsagua_menu/bombeos"
				action-command="bombeos"
				position="4"/>
			<menu text="fonsagua_menu/captaciones"
				action-command="captaciones"
				position="5"/>
			<menu text="fonsagua_menu/centros_educativos"
				action-command="centros_educativos"
				position="6"/>
			<menu text="fonsagua_menu/centros_salud"
				action-command="centros_salud"
				position="7"/>
			<menu text="fonsagua_menu/comunidades"
				action-command="comunidades"
				position="8"/>
			<menu text="fonsagua_menu/dep_distribucion"
				action-command="dep_distribucion"
				position="9"/>
			<menu text="fonsagua_menu/dep_intermedios"
				action-command="dep_intermedios"
				position="10"/>
			<menu text="fonsagua_menu/fuentes"
				action-command="fuentes"
				position="11"/>
			<menu text="fonsagua_menu/fuentes_contaminacion"
				action-command="fuentes_contaminacion"
				position="12"/>
			<menu text="fonsagua_menu/otros_servicios"
				action-command="otros_servicios"
				position="13"/>
			<menu text="fonsagua_menu/puntos_viviendas"
				action-command="puntos_viviendas"
				position="14"/>
			<menu text="fonsagua_menu/tuberias"
				action-command="tuberias"
				position="15"/>
		</extension>

		<extension class-name="es.udc.cartolab.gvsig.fonsagua.AutoFormsExtension" active="true">
			<tool-bar name="fonsagua">
				<selectable-tool icon="auto-forms" tooltip="enable_auto_forms"
					action-command="auto-forms" position="60" group="fonsagua" name="auto-forms" />
			</tool-bar>
		</extension>
		<extension class-name="es.udc.cartolab.gvsig.fonsagua.EmptyExtension" active="true">
			<tool-bar name="fonsagua">
				<selectable-tool icon="auto-forms" tooltip="disable_auto_forms"
					action-command="empty" position="60" group="fonsagua" name="auto-forms" />
			</tool-bar>
		</extension>
		
	</extensions>
</plugin-config>
