<?xml version="1.0" encoding="UTF-8"?>
<LAYER alias="datos_consumo">
       <TABLENAME>datos_consumo</TABLENAME>
       <PRIMARYKEY>
	<PKELEMENT>gid</PKELEMENT>
       </PRIMARYKEY>

       <RECORDSET>
       	    <FIELD>
		<FIELDNAME>cod_comunidad</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>cod_abastecimiento</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>fonsagua</DRDBSCHEMA>
			<DRDBTABLE>r_abastecimientos_comunidades</DRDBTABLE>
			<DRDBCOLUMNALIAS>cod_abastecimiento</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>cod_abastecimiento</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>cod_comunidad</DRDBFOREIGNKEY>
		</DOMAINREADER>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>tipo_abastecimiento</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>dominios</DRDBSCHEMA>
			<DRDBTABLE>tipo_abastecimiento</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
		</DOMAINREADER>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>mujeres</FIELDNAME>
		<ENABLEIF>
			<CONDITION>
				<COMPONENT>tipo</COMPONENT>
				<VALUE>Cantareras</VALUE>
			</CONDITION>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>hombres</FIELDNAME>
		<ENABLEIF>
			<CONDITION>
				<COMPONENT>tipo</COMPONENT>
				<VALUE>Cantareras</VALUE>
			</CONDITION>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>ninhas</FIELDNAME>
		<ENABLEIF>
			<CONDITION>
				<COMPONENT>tipo</COMPONENT>
				<VALUE>Cantareras</VALUE>
			</CONDITION>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>ninhos</FIELDNAME>
		<ENABLEIF>
			<CONDITION>
				<COMPONENT>tipo</COMPONENT>
				<VALUE>Cantareras</VALUE>
			</CONDITION>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>tiempo</FIELDNAME>
		<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		<ENABLEIF>
			<CONDITION>
				<COMPONENT>tipo</COMPONENT>
				<VALUE>Cantareras</VALUE>
			</CONDITION>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>consumo</FIELDNAME>
		<VALIDATIONRULE>DOUBLE_POSITIVE</VALIDATIONRULE>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>n_miembros</FIELDNAME>
		<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
	    </FIELD>
       </RECORDSET>

</LAYER>
