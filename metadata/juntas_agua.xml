	    </FIELD>
       	    <FIELD>
		<FIELDNAME>antiguedad</FIELDNAME>
		<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>nit</FIELDNAME>
		<ENABLEIF>
			<COMPONENT>per_juridica</COMPONENT>
			<VALUE>true</VALUE>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>anho_per_juridica</FIELDNAME>
		<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		<ENABLEIF>
			<COMPONENT>per_juridica</COMPONENT>
			<VALUE>true</VALUE>
		</ENABLEIF>
	    </FIELD>
       	    <FIELD>
		<FIELDNAME>nom_red_juntas</FIELDNAME>
		<ENABLEIF>
