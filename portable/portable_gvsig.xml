<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>./_fwAndami/andami.jar</jar>
  <outfile>./_fwAndami/gvsig-desktop.exe</outfile>
  <errTitle>gvSIG launch error</errTitle>
  <cmdLine>gvSIG gvSIG/extensiones -language pt</cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>http://www.gvsig.org/web/home/community/mailing-lists</supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>./install/launch4j/images/ico-gvSIG.ico</icon>
  <var>PROJ_LIB=./gvSIG/extensiones/org.gvsig.crs/data</var>
  <var>GDAL_DATA=./data/gdal</var>
  <classPath>
    <mainClass>com.iver.andami.Launcher</mainClass>
    <cp>./andami.jar</cp>
  </classPath>
  <jre>
    <path>../jre</path>
    <minVersion>1.5.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>1024</maxHeapSize>
    <opt>-XX:MaxPermSize=96M</opt>
    <opt>-DgvSIG.confDir=../cfg</opt>
  </jre>
  <versionInfo>
    <fileVersion>1.12.0.1417</fileVersion>
    <txtFileVersion>1.12.0 build 1417</txtFileVersion>
    <fileDescription>gvSIG desktop 1.12.0</fileDescription>
    <copyright>(c) 2009-2012 gvSIG Association</copyright>
    <productVersion>1.12.0.1417</productVersion>
    <txtProductVersion>1.12.0 build 1417</txtProductVersion>
    <productName>gvSIG desktop</productName>
    <companyName>gvSIG Association</companyName>
    <internalName>gvsig-desktop</internalName>
    <originalFilename>gvsig-desktop.exe</originalFilename>
  </versionInfo>
</launch4jConfig>