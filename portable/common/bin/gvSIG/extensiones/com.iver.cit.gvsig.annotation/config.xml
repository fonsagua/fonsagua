<?xml version="1.0" encoding="UTF-8"?>
<plugin-config>
    <depends plugin-name="com.iver.cit.gvsig"/>
    <depends plugin-name="com.iver.cit.gvsig.cad"/>
    <depends plugin-name="org.gvsig.extended-symbology"/>
    <resourceBundle name="text"/>
    <libraries library-dir="lib"/>
    <extensions>
        <extension
            class-name="com.iver.cit.gvsig.Annotation_AboutExtension"
            priority="1" active="false" description="Annotation about extension"/>
        <extension class-name="com.iver.cit.gvsig.Annotation_Extension"
            priority="50" active="true" description="Annotation extension">
            <menu action-command="ANNOTATIONLAYER"
                tooltip="create_annotation_layer"
                text="Capa/export_to/annotation" position="50350"/>
        </extension>
        <extension
            class-name="com.iver.cit.gvsig.Annotation_ToolExtension"
            priority="2" active="true" description="Annotation tool extension">
            <menu action-command="MODIFYANNOTATION"
                icon="ext-annotation" tooltip="modify_annotation" text="Capa/modify_annotation"/>
        </extension>
        <extension
            class-name="org.gvsig.tools.annotations.labeling.SingleLabelingExtension"
            priority="20" active="true" description="Extension to show and customize labels that will                 pop up over the feature that is clicked over                 (available since v2.0)."/>
    </extensions>
</plugin-config>
