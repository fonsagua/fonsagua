<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
	<depends plugin-name="com.iver.cit.gvsig"/>
	<depends plugin-name="com.iver.cit.gvsig.cad"/>
	<depends plugin-name="com.iver.cit.gvsig.wms"/>
	<depends plugin-name="es.udc.cartolab.gvsig.users"/>
	<depends plugin-name="es.icarto.gvsig.commons"/>
	<libraries library-dir="lib"/>
	<resourceBundle name="text"/>
	<label-set class-name="com.iver.cit.gvsig.project.documents.view.gui.View">
	<label id="units" size="75"/>
	<label id="x" size="120"/>
	<label id="y" size="120"/>
	<label id="4" size="110"/>
	<label id="5" size="110"/>
	<label id="distancearea" size="30"/>
	<label id="projection" size="110"/>
	<label id="constants" size="140"/>
	</label-set>
	<extensions>
        <extension
            class-name="es.udc.cartolab.gvsig.elle.LoadMapExtension"
            active="true" description="Load map">
            <!--
            <menu action-command="LoadMap" icon="load-map"
                text="fonsagua_tools_menu/data_base_menu/Load_map" position="31060"/>
            -->
        </extension>
        <extension
            class-name="es.udc.cartolab.gvsig.elle.SaveMapExtension"
            active="false" description="save_map">
            <menu action-command="SaveMap" icon="save-map"
                text="fonsagua_tools_menu/data_base_menu/save_map" position="31061"/>
        </extension>
        <extension class-name="es.udc.cartolab.gvsig.elle.DeleteMapExtension" 
        	description="delete_map" active="false">
			<menu text="fonsagua_tools_menu/data_base_menu/delete_map" icon="delete-map"
				action-command="DeleteMap" position="31062" />
		</extension>
        <extension
            class-name="es.udc.cartolab.gvsig.elle.LoadAllLegendsExtension"
            active="false" description="Load_several_legends">
            <menu action-command="LOADLEGENDS" icon="load-legends"
                text="fonsagua_tools_menu/data_base_menu/load_legends" position="31063"/>
        </extension>
        <extension
            class-name="es.udc.cartolab.gvsig.elle.SaveAllLegendsExtension"
            active="false" description="Save_several_legends">
            <menu action-command="ALL" icon="save-all-legends"
                text="fonsagua_tools_menu/data_base_menu/Save_legends" position="31064"/>
        </extension>
    </extensions>
</plugin-config>
